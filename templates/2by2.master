{% extends "site.master" %}
{% block body %}
	<div class="containery">
		<div class="row-fluid">
			<div class="span6 side">
				{% block left %}{% endblock left %}
			</div>
			<div class="span6 side">
				{% block right %}{% endblock right %}
			</div>
		</div>
	</div>
	<div class="containery" id="bottombar">
		{% block extracontent %}{% endblock extracontent %}
	</div>
	<div style="height: 180px;"></div>
{% endblock body %}